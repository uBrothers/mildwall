<!DOCTYPE html>
<html>
<head>
  <link rel="shortcut icon" type="image" href="data:image/png;base64,"/>
  <% include ../partials/head %>
  <link rel="stylesheet" type="text/css" href="css/main.css"/>
  <style>
    h, p1{
      font-family:Gothic, sans-serif;
      font-size:17px;
      line-height:150%;
      margin:0;
      padding:0;
    }
    .normalHead {
      border:0;
      border-bottom:1px solid #fa9200;
      background-color:#ffb347;
      width:100%;
      height:auto;
      float:left;
      overflow:hidden;
    }
    .aboutBox {
      position:relative;
      overflow:auto;
      background-color: white;
      display:block;
      width:50%;
      height:370px;
      margin-right:auto;
      margin-left:auto;
      text-align:left;
      border-radius:5px;
      -webkit-box-shadow: 0px 2px 4px 1px #a6a6a6;
      -moz-box-shadow: 0px 2px 4px 1px #a6a6a6;
      box-shadow: 0px 2px 4px 1px #a6a6a6;
    }
    #selectBox {
      position:relative;
      overflow:auto;
      display:block;
      width:40%;
      margin:0 auto;
      text-align:center;
    }
    .introBox {
      display:none;
      overflow:auto;
      position:absolute;
      top:0;
      width:100%;
      height:370px;
      padding:50px 50px;
    }
    #dirRight, #dirLeft {
      position:absolute;
      z-index:2;
      border-radius:5px;
      width:50px;
      height:50px;
      bottom:0;
      background:#edebe9;
      cursor:pointer;
      padding:0;
      -webkit-transition-duration: 0.3s; /* Safari */
      transition-duration: 0.3s;
    }
    #dirRight:hover, #dirLeft:hover {
      background:#bab3ab;
    }

    #dirRight {
      right:0px;
    }
    #dirLeft {
      left:0px;
    }
    #login {
      display:block;
      font-size:16px;
      width:50%;
      color:#402805;
      background-color:none;
      border:3px solid #402805;
      border-radius:3px;
      padding:7px 10px;
      margin:50px auto;
      -webkit-transition-duration: 0.3s; /* Safari */
      transition-duration: 0.3s;
      text-align:center;
    }
    #login:hover {
      color:#edebe9;
      background-color: #402805;
      text-decoration:none;
    }
    .selectButton{
      width:17px;
      height:17px;
      margin:3px;
      margin-top:20px;
      border:0;
      border-radius:3px;
      background:#edebe9;
    }
    .selectButton:hover{
      background:#bab3ab!important;
    }

    @media only screen and (max-width: 1035px) {
        h, p1{
          font-size:15px;
        }
        .aboutBox {
          width:90%;
        }
        #selectBox {
          width:240px;
        }
        #slt9 {
          clear:both;
        }
        .introBox {
          padding:20px 20px;
        }
    }
  </style>
  <script>
  $(document).ready(function(){
    var dirRight = $("#dirRight");
    var dirLeft = $("#dirLeft");
    var box1 = $("#box1");
    var box2 = $("#box2");
    var selectBox = $("#selectBox");
    var pageNum = 1;
    goToSpecial();
    dirRight.click(function(){
      backToNormal();
      if(pageNum==16){
        pageNum=0;
      }
      pageNum++;
      goToSpecial();
    });
    dirLeft.click(function(){
      backToNormal();
      if(pageNum==1){
        pageNum=17;
      }
      pageNum--;
      goToSpecial();
    });
    selectBox.on("click", ".selectButton", function(){
      backToNormal();
      pageNum=this.value;
      goToSpecial();
    });
    function backToNormal() {
      $("#box"+pageNum).fadeOut();
      $("#slt"+pageNum).css("background","#edebe9");
    }
    function goToSpecial(){
      $("#box"+pageNum).fadeIn();
      $("#slt"+pageNum).css("background","#bab3ab");
    }
  });
  </script>
</head>
<body>
  <div class="normalHead">
